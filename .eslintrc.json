{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "semi": ["error", "never"],
    "indent": ["error", 4],
    "prettier/prettier": ["error"]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  }
}
